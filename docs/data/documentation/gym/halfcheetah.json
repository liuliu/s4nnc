aragraph"}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"5"}]}],[{"type":"paragraph","inlineContent":[{"text":"velocity of the tip along the y-axis","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"-Inf","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"Inf","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"fthigh","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"hinge"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"angle (rad)"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"6"}]}],[{"type":"paragraph","inlineContent":[{"text":"angular velocity of front tip","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"-Inf"}]}],[{"type":"paragraph","inlineContent":[{"text":"Inf","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"fshin","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"hinge"}]}],[{"type":"paragraph","inlineContent":[{"text":"angle (rad)","type":"text"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"7"}]}],[{"type":"paragraph","inlineContent":[{"text":"angular velocity of second rotor","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"-Inf","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Inf"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"ffoot"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"hinge"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"angle (rad)"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"8","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"x-coordinate of the front tip","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"-Inf","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"Inf","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"rootx","type":"text"}]}],[{"type":"p