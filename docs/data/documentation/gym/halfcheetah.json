text"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Inf"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"bshin"}],"type":"paragraph"}],[{"inlineContent":[{"text":"hinge","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"angle (rad)","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"4"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"velocity of the tip along the x-axis"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"-Inf"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Inf","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"bfoot","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"hinge"}],"type":"paragraph"}],[{"inlineContent":[{"text":"angle (rad)","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"5","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"velocity of the tip along the y-axis","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"-Inf","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Inf"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"fthigh"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"hinge"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"angle (rad)"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"6"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"angular velocity of front tip"}],"type":"paragraph"}],[{"inlineContent":[{"text":"-Inf","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Inf","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"fshin","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"hinge"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"angle (rad)"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"7","type":"text"}],"type":"paragraph"}],[{"inli